{
  "metadata": [
    {
      "src": [
        {
          "files": [ "*/*.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ],
          "src": "repos/API8/internals/src"
        }
      ],
      "dest": "obj/internals/api"
    }
  ],
  "build": {
    "content": [
      {
        "version": "internals",
        "files": [ "**/*.yml" ],
        "src": "obj/internals",
        "dest": "internals",
        "rootTocPath": "toc.html"
      },
      {
        "version": "internals",
        "files": [ "**/*.{yml,md}" ],
        "src": "repos/API8/internals/docs",
        "dest": "internals",
        "rootTocPath": "toc.html"
      },
      {
        "files": [ "*.yml", "*.md" ]
      }
    ],
    "overwrite": [
      {
        "version": "internals",
        "files": "*/doc/api/**.md",
        "src": "repos/API8/internals/src"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "./templates/material",
      "./templates/tizen"
    ],
    "globalMetadata": {
        "_appLogoPath" : "images/tizen_logo.png",
        "_appFaviconPath" : "images/tizen_favicon.ico",
        "_appFooter" : "<span>Copyright &copy; 2016-2020 Samsung <br> Generated by <strong>DocFX</strong></span>",
        "_disableAffix": false,
        "_disableContribution": false,
        "_baseUrl": "https://samsung.github.io/TizenFX/master/",
        "_enableSearch" : "true"
    },
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}